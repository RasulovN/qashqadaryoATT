{{!-- NEWS POST page content --}}
<section class="page-section portfolio home-sections" id="blog">
    <div class="container">
        <h2 class="page-section-heading text-center text-uppercase  mb-0">YANGILIKLAR</h2>
        <div class="row justify-content-center mt-4">
            {{#if blogs}}
            {{#each blogs}}  
            <div class="col-md-6 col-lg-4 mb-5">
                 <a class="article-card-news" href="/blogpost?post={{slugUrl}}">
                        <div class="article-card__thumbnail">
                            <img src="uploads/{{image}}" alt="{{title}}">
                        </div>
                        <div class="article-card__content">
                            <h2 class="article-card__title">{{title}} </h2>
                            <div class="article-card__meta mt-3">
                                <span class="article-card__timestamp"><i class="ion-clock"></i> {{createdAt}}</span>
                            </div>
                        </div>
                    </a>
            </div>
            {{/each}}
            <div class="text-center">
                <a href="/blogs" class=" btn-outline-secondary all-new-btn">Barchasi</a>
            </div>
            {{else}}
            <div class="alert alert-danger">
                <h3>Yangiliklar mavjud emas</h3>
            </div>
            {{/if}}
        </div>
    </div>
</section>
{{!-- Blog page content end--}}



